{
  "name": "@loopback/openapi-v3",
  "description": "Decorators that annotate LoopBack artifacts with OpenAPI v3 metadata and utilities that transform LoopBack metadata to OpenAPI v3 specifications",
  "version": "8.0.1",
  "keywords": [
    "Swagger",
    "OpenAPI Spec",
    "TypeScript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/openapi-v3"
  },
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-openapi-v3*.tgz dist *.tsbuildinfo package",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-openapi-v3*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^4.0.1"
  },
  "dependencies": {
    "@loopback/repository-json-schema": "^6.0.1",
    "debug": "^4.3.4",
    "http-status": "^1.5.2",
    "json-merge-patch": "^1.0.2",
    "lodash": "^4.17.21",
    "openapi3-ts": "^2.0.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.1",
    "@loopback/core": "^4.0.1",
    "@loopback/eslint-config": "^13.0.1",
    "@loopback/openapi-spec-builder": "^5.0.1",
    "@loopback/repository": "^5.0.1",
    "@loopback/testlab": "^5.0.1",
    "@types/debug": "^4.1.7",
    "@types/json-merge-patch": "0.0.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.18.21"
  },
  "gitHead": "ccf7863d0639de966d6c6129b4aa94ba1dce2c33"
}
